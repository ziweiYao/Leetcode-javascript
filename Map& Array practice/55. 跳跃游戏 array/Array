var canJump1 = function(nums) {
    let current_step = 0;
    let StepCanGo = nums[current_step];
    while( current_step <= nums.length -1 ){
        if(current_step === nums.length -1){
            return true;
        }
        if(StepCanGo <= 0){
            return false;
        }
        if(nums[current_step]>StepCanGo){
            StepCanGo = nums[current_step]
        }
        console.log("current_step: ",current_step,"StepCanGo: ", StepCanGo)    
        current_step += 1;
        StepCanGo -= 1;
    }
    return false
};

//官网答案↓
var canJump = function(nums) {
    let mx = 0;
    for (let i = 0; mx < nums.length - 1; i++) {
        if (i > mx) { // 无法到达 i
            return false;
        }
        mx = Math.max(mx, i + nums[i]); // 从 i 最右可以跳到 i + nums[i]
        if(mx >= nums.length){
            return true;
        }
    }
    return true;
};
